<Window x:Class="ATMProject.WithdrawErrorPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATMProject"
        mc:Ignorable="d"
        WindowStyle="None"
        WindowStartupLocation="CenterScreen"
        AllowsTransparency="True"
        Title="WithdrawErrorPopup" Height="400" Width="800" ContentRendered="Window_ContentRendered">
    <Grid Background="White">
        <Border BorderBrush="#FFE42F2F" BorderThickness="20" />
        <Image Source="Assests/Icons/Warning.png" Margin="20" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Width="250" Height="250"></Image>
    </Grid>
</Window>
